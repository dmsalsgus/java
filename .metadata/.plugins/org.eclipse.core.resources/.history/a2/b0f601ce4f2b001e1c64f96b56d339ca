package com.test.question;

public class Q060 {
	public static void main(String[] args) {
		
		int num=100;
		
		fibo(num);
	}
	public static int fibo(int num) {
		int result;
		if(num<=1) {
			result=1;
			System.out.print(result);
		}else {
			result=fibo(num-2) + fibo(num-1);
			System.out.print(result);
		}
		return 0;
	}
}
