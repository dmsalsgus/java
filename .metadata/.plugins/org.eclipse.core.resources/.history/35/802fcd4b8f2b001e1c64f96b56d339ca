		System.out.print("값 : ");
		int value=sc.nextInt();
		
		System.out.print(Arrays.toString(original));
		System.out.println();
		for(int i=0; i<original.length; i++) {
			if(i==location) {
				for(int j=location+1; j<original.length; j++) {
					original[original.length-ij]=original[original.length-j-1];
				}
				original[i]=value;
			}else continue;
		}
		
