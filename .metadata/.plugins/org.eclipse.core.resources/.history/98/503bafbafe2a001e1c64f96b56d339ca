package com.test.question;
import java.util.*;

public class Q056 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		System.out.print("첫번째 숫자 : ");
		int num1=sc.nextInt();
		System.out.print("두번째 숫자 : ");
		int num2=sc.nextInt();
		
		divisor(num1);
		divisor(num2);
		
	}
	private static void divisor(int num) {  //약수 구하는 메소드
		for(int i=1; i<=num; i++) {
			if(num%i==0) {
				System.out.printf("%d, ",i);
			}
		}
		System.out.println();
	}
	
}
