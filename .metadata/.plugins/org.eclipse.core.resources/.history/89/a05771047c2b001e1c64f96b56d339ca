package com.test.question;
import java.util.*;

public class Q061 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] nums=new int[5];
		for(int i=0; i<5; i++) {
			System.out.print("숫자 : ");
			nums[i]=sc.nextInt();
			for(int j=5; j>0; j--) {
				System.out.print(nums[i]);
			}
		}
	}
}
