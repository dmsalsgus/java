package com.test.question;

public class Q063 {
	public static void main(String[] args) {

		int[] list = new int[4];
		list[0]=10;
		list[1]=20;
		list[2]=30;
		list[3]=40;
		String result=dump(list);
		System.out.printf("nums = %s\n",result);

	}
	public static String dump(int[] list) {
		String txt="[";
		for(int i=0; i<4; i++) {
			txt+=list[i];
			if(i==3) continue;
			txt+=",";
		}
		txt+="]";
		return txt;
	}
}
